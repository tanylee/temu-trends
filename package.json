{
  "name": "temu-trends",
  "version": "1.0.0",
  "description": "Automated TEMU affiliate + Pinterest trends pipeline with Oxylabs and Netlify integration.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "scrape": "node scripts/scrape_temu.js",
    "trends": "node scripts/collect_trends.js && node scripts/aggregate_trends.js",
    "seasonal": "node scripts/collect_seasonal.js",
    "regional": "node scripts/collect_trends_regions.js",
    "validate": "node scripts/validate_temu_links.js",
    "post": "node scripts/post_pinterest_pins.js",
    "build": "node scripts/build_site.js",
    "deploy": "netlify deploy --prod",
    "clean": "rm -rf node_modules && npm install"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "luxon": "^3.5.0",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.4.1",
    "playwright": "^1.45.3",
    "puppeteer": "^22.12.1",
    "random-useragent": "^0.5.0",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "eslint": "^9.10.0",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "temu",
    "affiliate",
    "pinterest",
    "automation",
    "netlify",
    "oxylabs",
    "scraper",
    "trends"
  ],
